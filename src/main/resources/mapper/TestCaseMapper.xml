<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.okay.family.mapper.SaveMapper">

    <sql id="BASE_TABLE">
        family_test_user
    </sql>


    <!--<insertDetail >-->
    <insert id="saveCase" parameterType="com.okay.family.common.bean.testcase.TestCaseBean">
        INSERT INTO
        <include refid="BASE_TABLE"/>
        (ID,HISTORYID,ACCOUNT,PWD,TOKEN,COOKIES)
        values (#{id},#{historyId},#{account},#{pwd},#{token},#{cookies})
    </insert>


</mapper>