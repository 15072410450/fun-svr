<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.okay.family.mapper.SaveMapper">

    <sql id="save_table">
        qa_request_record
    </sql>

    <insert id="saveRequest" parameterType="com.okay.family.common.bean.RequestSaveBean">
        INSERT INTO
        <include refid="save_table"/>
        (uid,type,method,host,api,status,code,expendtime,datasize,requestid)
        values (#{uid},#{type},#{method},#{host},#{api},#{status},#{code},#{expendtime},#{datasize},#{requestid})
    </insert>


</mapper>